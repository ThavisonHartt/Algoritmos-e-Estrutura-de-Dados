Algoritmo "Tipos de Variáveis"
Var
Modelo: Caractere
Ano: Inteiro
Valor: Real
IpvaPago: Logico
SeguroPago: Logico
Inicio
Modelo <- "Gol"
Ano <- 2010
Valor <- 20000.15
IpvaPago <- FALSO
SeguroPago <- VERDADEIRO

Escreval(Modelo)
Escreval(Ano)
Escreval(Valor)
Escreval(IpvaPago)
Escreval(SeguroPago)
Fimalgoritmo